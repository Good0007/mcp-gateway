# Docker 环境变量配置示例
# 复制此文件为 .env 并根据需要修改

# 服务器配置
NODE_ENV=production
PORT=3000
HOST=0.0.0.0

# MCP 代理 Token（如果需要认证）
# MCP_SERVER_TOKEN=your-secure-token-here

# 日志级别
LOG_LEVEL=info

# 其他自定义配置
# API_TIMEOUT=30000
# MAX_CONNECTIONS=100

# 是否启用登录认证
MCP_AGENT_AUTH_ENABLE=true
# 登录用户名
MCP_AGENT_USERNAME=admin
# 登录密码（请务必修改为强密码！）
MCP_AGENT_PASSWORD=admin123
